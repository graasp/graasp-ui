{
  "name": "@graasp/ui",
  "version": "5.0.1",
  "description": "React components for the Graasp ecosystem.",
  "author": "Graasp",
  "contributors": [
    "Juan Carlos Farah",
    "Kim Lan Phan Hoang",
    "Abdallah Al Chami",
    "Alexandre Chau",
    "Basile Spaenlehauer",
    "Jérémy La Scala",
    "Thibault Reidy"
  ],
  "license": "AGPL-3.0-only",
  "repository": {
    "url": "https://github.com/graasp/graasp-ui"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./text-editor": {
      "types": "./dist/text-editor.d.ts",
      "import": "./dist/text-editor.js",
      "default": "./dist/text-editor.js"
    },
    "./apps": {
      "types": "./dist/apps.d.ts",
      "import": "./dist/apps.js",
      "default": "./dist/apps.js"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "prepack": "yarn build",
    "prepare": "yarn prepack && yarn hooks:install",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "check": "yarn prettier:check && yarn lint && yarn type-check",
    "prettier:check": "prettier --check \"{src,example}/**/*.{js,ts,tsx}\"",
    "prettier:write": "prettier --write \"{src,example}/**/*.{js,ts,tsx}\"",
    "analyze-bundle": "npx vite-bundle-visualizer",
    "test": "yarn test:unit-ci && yarn test:sb",
    "test:sb": "yarn storybook:build && concurrently -k -s first -n \"SB, TEST\" -c \"magenta,blue\" \"yarn storybook:preview\" \"yarn storybook:test-ci\"",
    "test:unit": "vitest --passWithNoTests",
    "test:unit-ci": "vitest run --passWithNoTests",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "post-install": "yarn hooks:install",
    "post-commit": "git status",
    "pre-commit": "yarn check",
    "pre-push": "yarn build",
    "storybook": "storybook dev -p 65000",
    "storybook:build": "storybook build",
    "storybook:preview": "http-server storybook-static --port 6006 --silent",
    "storybook:test": "test-storybook --coverage",
    "storybook:test-ci": "wait-on http://127.0.0.1:6006 && yarn storybook:test"
  },
  "dependencies": {
    "http-status-codes": "2.3.0",
    "interweave": "13.1.0",
    "lodash.truncate": "4.4.2",
    "react-cookie-consent": "9.0.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-quill": "2.0.0",
    "react-rnd": "10.4.12",
    "uuid": "10.0.0"
  },
  "peerDependencies": {
    "@emotion/cache": "~11.10.7 || ~11.11.0 || ~11.13.0",
    "@emotion/react": "~11.10.6 || ~11.11.0 || ~11.13.0",
    "@emotion/styled": "~11.10.6 || ~11.11.0 || ~11.13.0",
    "@graasp/sdk": "^4.29.0",
    "@graasp/stylis-plugin-rtl": "^2",
    "@graasp/translations": "^1.23.0",
    "@mui/icons-material": "~5.14.0 || ~5.15.0 || ~5.16.0",
    "@mui/lab": "~5.0.0-alpha.150",
    "@mui/material": "~5.14.0 || ~5.15.0 || ~5.16.0",
    "i18next": "^22.4.15 || ^23.0.0",
    "katex": "0.16.11",
    "lucide-react": "^0.417.0 || ^0.429.0 || ^0.436.0 || ^0.439.0 || ^0.441.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^13.0.0 || ^14.0.0 || ^15.0.0",
    "react-router-dom": "^6.11.0",
    "stylis": "^4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@emotion/cache": "~11.13.1",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.10.0",
    "@graasp/sdk": "4.29.1",
    "@graasp/stylis-plugin-rtl": "2.2.0",
    "@mui/icons-material": "5.16.7",
    "@mui/lab": "5.0.0-alpha.173",
    "@mui/material": "5.16.7",
    "@storybook/addon-a11y": "8.3.1",
