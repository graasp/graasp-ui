{
  "name": "@graasp/ui",
  "version": "4.8.1",
  "description": "React components for the Graasp ecosystem.",
  "author": "Graasp",
  "contributors": [
    "Juan Carlos Farah",
    "Kim Lan Phan Hoang",
    "Abdallah Al Chami",
    "Alexandre Chau",
    "Basile Spaenlehauer"
  ],
  "license": "AGPL-3.0-only",
  "repository": "graasp/graasp-ui",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./table": {
      "types": "./dist/table.d.ts",
      "import": "./dist/table.js",
      "default": "./dist/table.js"
    },
    "./text-editor": {
      "types": "./dist/text-editor.d.ts",
      "import": "./dist/text-editor.js",
      "default": "./dist/text-editor.js"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc --noEmit -p tsconfig.build.json && vite build",
    "build:ts": "tsc && tsc-alias",
    "build:tsup": "yarn tsup --entry src/index.ts --entry src/table.ts --dts --format esm,cjs --clean",
    "prepack": "yarn build",
    "prepare": "yarn prepack && yarn hooks:install",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "check": "yarn prettier:check && yarn lint && yarn type-check",
    "prettier:check": "prettier --check \"{src,example}/**/*.{js,ts,tsx}\"",
    "prettier:write": "prettier --write \"{src,example}/**/*.{js,ts,tsx}\"",
    "analyze-bundle": "npx vite-bundle-visualizer",
    "test": "yarn test:unit-ci && yarn test:sb",
    "test:sb": "yarn storybook:build && concurrently -k -s first -n \"SB, TEST\" -c \"magenta,blue\" \"yarn storybook:preview\" \"yarn storybook:test-ci\"",
    "test:unit": "vitest",
    "test:unit-ci": "vitest run",
    "test:watch": "jest src --watchAll --env=jsdom",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "post-install": "yarn hooks:install",
    "post-commit": "git status",
    "pre-commit": "yarn check",
    "pre-push": "yarn build",
    "storybook": "storybook dev -p 65000",
    "storybook:build": "storybook build",
    "storybook:preview": "http-server storybook-static --port 6006 --silent",
    "storybook:test": "test-storybook --coverage",
    "storybook:test-ci": "wait-on http://127.0.0.1:6006 && yarn storybook:test"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "31.1.1",
    "@ag-grid-community/react": "^31.1.1",
    "@ag-grid-community/styles": "^31.1.1",
    "@storybook/react-vite": "7.6.17",
    "http-status-codes": "2.3.0",
    "interweave": "13.1.0",
    "katex": "0.16.9",
    "lodash.truncate": "4.4.2",
    "react-cookie-consent": "9.0.0",
    "react-quill": "2.0.0",
    "react-rnd": "10.4.1",
    "react-text-mask": "5.5.0",
    "uuid": "9.0.1",
    "vitest": "1.3.1"
  },
  "peerDependencies": {
    "@emotion/cache": "~11.10.7 || ~11.11.0",
    "@emotion/react": "~11.10.6 || ~11.11.0",
    "@emotion/styled": "~11.10.6 || ~11.11.0",
    "@graasp/sdk": "^4.0.0",
    "@graasp/translations": "^1.23.0",
    "@mui/icons-material": "~5.14.0 || ~5.15.0",
    "@mui/lab": "~5.0.0-alpha.150",
    "@mui/material": "~5.14.0 || ~5.15.0",
    "i18next": "^22.4.15 || ^23.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^13.0.0 || ^14.0.0",
    "react-router-dom": "^6.11.0",
    "stylis": "^4.1.3",
    "stylis-plugin-rtl": "^2.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@emotion/cache": "~11.11.0",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@graasp/sdk": "4.0.1",
    "@mui/icons-material": "5.15.11",
    "@mui/lab": "5.0.0-alpha.157",
    "@mui/material": "5.15.11",
    "@storybook/addon-a11y": "7.6.17",
    "@storybook/addon-actions": "7.6.17",
    "@storybook/addon-coverage": "1.0.0",
    "@storybook/addon-docs": "7.6.17",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/addon-interactions": "7.6.17",
    "@storybook/addon-links": "7.6.17",
    "@storybook/jest": "0.2.3",
    "@storybook/react": "7.6.17",
    "@storybook/test-runner": "0.16.0",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/jest": "29.5.12",
    "@types/katex": "^0.16.7",
    "@types/lodash.truncate": "4.4.9",
    "@types/node": "20.11.20",
    "@types/react": "18.2.58",
    "@types/react-dom": "18.2.19",
    "@types/react-router-dom": "5.3.3",
    "@types/react-text-mask": "5.4.14",
    "@types/stylis": "4.2.5",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "@vitejs/plugin-react": "4.2.1",
    "concurrently": "8.2.2",
    "cross-env": "7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "http-server": "14.1.1",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-dom": "4.0.0",
    "jest-environment-jsdom": "29.7.0",
    "jsdom": "24.0.0",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "14.0.5",
    "react-query": "3.39.3",
    "react-router-dom": "6.22.1",
    "react-test-renderer": "18.2.0",
    "sass": "1.71.1",
    "storybook": "7.6.17",
    "stylis": "^4.3.1",
    "stylis-plugin-rtl": "^2.1.1",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "vite": "5.1.4",
    "vite-plugin-dts": "3.7.3",
    "wait-on": "7.2.0"
  },
  "packageManager": "yarn@4.1.0"
}
