{
  "name": "@graasp/ui",
  "version": "0.8.0",
  "type": "module",
  "description": "React components for the Graasp ecosystem.",
  "author": "graasp",
  "contributors": [
    "Juan Carlos Farah",
    "Kim Lan Phan Hoang",
    "Abdallah Al Chami",
    "Alexandre Chau",
    "Basile Spaenlehauer"
  ],
  "license": "AGPL-3.0-only",
  "repository": "graasp/graasp-ui",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rimraf dist && tsc && rollup -c rollup.config.js",
    "start": "rimraf dist && tsc && rollup --watch -c rollup.config.js",
    "start:example": "yarn workspace graasp-ui-example start",
    "prepack": "yarn build",
    "lint": "eslint src",
    "prettier:check": "prettier --check \"{src,example}/**/*.{js,ts,tsx}\"",
    "prettier:write": "prettier --write \"{src,example}/**/*.{js,ts,tsx}\"",
    "pretest": "cd example && yarn",
    "test": "yarn storybook:test",
    "test:unit": "cross-env CI=1 jest src --env=jsdom",
    "test:watch": "jest src --watchAll --env=jsdom",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "prepublishOnly": "pinst --disable",
    "postpack": "pinst --enable",
    "postpublish": "pinst --enable",
    "post-commit": "git status",
    "pre-commit": "yarn prettier:check && yarn lint",
    "storybook": "start-storybook -p 6006",
    "storybook:test": "test-storybook --coverage",
    "storybook:build": "build-storybook"
  },
  "dependencies": {
    "@graasp/sdk": "0.3.0",
